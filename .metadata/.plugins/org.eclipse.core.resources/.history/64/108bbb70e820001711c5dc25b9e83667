import java.util.Scanner;

public class FirstFunction {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int radius;
		double area;
		final double pi=3.142;
		
		Scanner readInput = new Scanner(System.in);
		
		//System.out.print("Enter the radious: ");
		
		radius = readInput.nextInt();
		
		area=pi*radius*radius;
		//System.out.printf("The area is: %f The PI is %f ", area, pi);
		System.out.printf("5195 is fun course");
		System.out.printf("\rHello Student\n");
		System.out.printf("First Name \tLast Name \tCity\n");

	}

}
